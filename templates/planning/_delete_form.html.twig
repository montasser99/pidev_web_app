<form method="post" action="{{ path('app_planning_delete', {'dated': planning.dated, 'idcir': planning.idcir.idcircuit, 'idmoy': planning.idmoy.idmoy}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ planning.dated) }}">
    <button class="btn">Delete</button>
</form>
